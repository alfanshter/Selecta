<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.alfanshter.jatimpark.ui.Tracking.TrackingFragment"
        android:label="@string/title_peta"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.alfanshter.jatimpark.ui.dashboard.DashboardFragment"
        android:label="@string/title_sharing"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.alfanshter.jatimpark.ui.JoinKode.NotificationsFragment"
        android:label="@string/title_invite"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_sharingmaps"
        android:name="com.alfanshter.jatimpark.ui.shareRombongan.ShareRombongan"
        android:label="Map Family"
        tools:layout="@layout/share_rombongan_fragment"
        />


    <fragment
        android:id="@+id/navigation_generateKode"
        android:name="com.alfanshter.jatimpark.ui.generateKode.BlankFragment"
        android:label="blank"
        tools:layout="@layout/fragment_blank"
        />

    <fragment
        android:id="@+id/navigation_generate"
        android:name="com.alfanshter.jatimpark.ui.generateKode.Generate"
        android:label="blank"
        tools:layout="@layout/fragment_home"
        />




</navigation>